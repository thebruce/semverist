{
  "name": "semverist",
  "version": "0.0.0-development",
  "description": "The Semverist provides a uniform interface for understanding and manipulating directories, config, and objects with a semver influenced hierarchy.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "nyc ava && nyc report --reporter=html  --reporter=lcov",
    "precommit": "npm run lint",
    "prepush": "npm test",
    "coverage": "nyc check-coverage --statements 97 --lines 97 --branches 97 --functions 97",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "documentation": "documentation build lib/** -f html -o docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thebruce/semverist.git"
  },
  "keywords": [
    "semver"
  ],
  "author": {
    "name": "David Diers",
    "url": "http://github.com/thebruce",
    "id": 590058
},
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thebruce/semverist/issues"
  },
  "homepage": "https://github.com/thebruce/semverist#readme",
  "dependencies": {
    "callsite": "^1.0.0",
    "config": "^1.25.1",
    "fs-extra": "^3.0.0",
    "klaw-sync": "1.1.2",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "molotov": "0.0.1",
    "require-directory": "^2.1.1",
    "semver": "^5.3.0",
    "semver-diff": "^2.1.0",
    "semver-utils": "^1.1.1",
    "version-comparison": "^2.1.1"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "coveralls": "^2.11.16",
    "emdaer": "^0.3.3",
    "documentation": "^4.0.0-beta.19",
    "eslint": "^3.16.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-find-rules": "^1.13.0",
    "eslint-plugin-ava": "^4.2.0",
    "eslint-plugin-import": "^2.0.1",
    "husky": "^0.13.1",
    "npm-run-all": "^4.0.1",
    "nyc": "^10.1.2",
    "semantic-release": "^6.3.2",
    "temp": "^0.8.3"
  }
}

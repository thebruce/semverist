{
  "name": "semverist",
  "version": "0.0.0-development",
  "description": "The Semverist provides a uniform interface for understanding and manipulating directories, config, and objects with a semver influenced hierarchy.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "jest --coverage",
    "precommit": "npm run lint",
    "prepush": "npm test",
    "coveralls": "cat ./__coverage__/lcov.info | coveralls",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "debug-test": "node --inspect --debug-brk ./node_modules/.bin/jest --runInBand --env jest-environment-node-debug",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "documentation": "documentation build lib/** -f html -o docs",
    "test-ci": "jest --coverage --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thebruce/semverist.git"
  },
  "keywords": [
    "semver"
  ],
  "author": {
    "name": "David Diers",
    "id": 590058,
    "url": "http://daviddiers.com"
  },
  "contributors": [
    {
      "name": "David Diers",
      "id": 590058,
      "url": "http://daviddiers.com"
    },
    {
      "name": "Elliott Foster",
      "id": 447151,
      "url": "http://codebrews.com"
    },
    {
      "name": "Flip",
      "url": "http://github.com/flipactual",
      "id": 1306968
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thebruce/semverist/issues"
  },
  "homepage": "https://github.com/thebruce/semverist#readme",
  "dependencies": {
    "config": "^2.0.0",
    "fs-extra": "5.0.0",
    "klaw-sync": "3.0.1",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "molotov": "^2.1.1",
    "npm": "^5.7.1",
    "semver": "^5.3.0",
    "semver-diff": "^2.1.0",
    "semver-utils": "^1.1.1",
    "version-comparison": "^2.1.1"
  },
  "devDependencies": {
    "@commitlint/config-angular": "^6.1.3",
    "babel-eslint": "^8.2.2",
    "commitlint": "^6.1.2",
    "condition-noop": "^1.0.0",
    "coveralls": "3.0.0",
    "documentation": "^6.1.0",
    "emdaer": "^0.3.3",
    "eslint": "^4.18.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "husky": "^0.14.3",
    "jest": "^22.1.4",
    "jest-environment-node-debug": "^2.0.0",
    "npm-run-all": "^4.0.1",
    "semantic-release": "^7.0.2",
    "temp": "^0.8.3",
    "validate-commit-message": "^3.0.1",
    "validate-commit-msg": "^2.14.0"
  },
  "jest": {
    "cacheDirectory": ".tmp/jest_cache",
    "coverageDirectory": "__coverage__",
    "coverageReporters": [
      "lcov",
      "text",
      "text-summary"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 97,
        "functions": 97,
        "lines": 97,
        "statements": 97
      }
    },
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/__tests__/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/__helpers__/",
      "<rootDir>/__tests__/.*/[__mocks__|files]/"
    ]
  },
  "release": {
    "verifyConditions": "condition-noop"
  }
}
